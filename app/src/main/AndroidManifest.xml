<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.firstone">

    <application
        android:allowBackup="true"
        android:icon="@drawable/math"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Result"></activity>

        <meta-data
            android:name="com.google.android.actions"
            android:resource="@drawable/buttonshape" />

        <activity
            android:name=".Splash"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Formula" />
        <activity android:name=".Chess" />
        <activity android:name=".Pop" />
        <activity android:name=".MainActivity" />
        <activity android:name=".Game_Page" />
        <activity android:name=".Game_Splash" />
    </application>

</manifest>